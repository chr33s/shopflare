{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "shopflare",
	"compatibility_date": "2024-11-01",
	"main": "./server.ts",
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist/client"
	},
	"dev": {
		"ip": "0.0.0.0",
		"port": 8080
	},
	"kv_namespaces": [
		{
			"binding": "SESSION_STORAGE",
			"id": "?"
		}
	],
	"logpush": true,
	"observability": {
		"enabled": true,
		"logs": {
			"invocation_logs": false
		}
	},
	"placement": {
		"mode": "smart"
	},
	"vars": {
		"NODE_VERSION": 22
	}
}
