{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "shopflare",
	"compatibility_date": "2025-06-17",
	"compatibility_flags": ["nodejs_als"],
	"main": "./app/entry.ssr.tsx",
	"assets": {
		"binding": "ASSETS",
		"directory": "./build/client"
	},
	"dev": {
		"ip": "0.0.0.0",
		"port": 8080
	},
	"logpush": true,
	"observability": {
		"enabled": true,
		"head_sampling_rate": 1
	},
	"placement": {
		"mode": "smart"
	},
	"routes": [
		{
			"custom_domain": true,
			"pattern": "?"
		}
	],
	"send_metrics": false,
	"upload_source_maps": true,
	"services": [
		{
			"binding": "RSC",
			"service": "shopflare-rsc"
		}
	],
	"workers_dev": false
}
