{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "shopflare",
	"compatibility_date": "2025-06-01",
	"main": "./server.ts",
	"assets": {
		"binding": "ASSETS",
		"directory": "./build/client"
	},
	"dev": {
		"ip": "0.0.0.0",
		"port": 8080
	},
	"kv_namespaces": [
		{
			"binding": "SESSION_KV",
			"id": "?"
		}
	],
	"logpush": true,
	"observability": {
		"enabled": true,
		"logs": {
			"invocation_logs": false
		}
	},
	"placement": {
		"mode": "smart"
	},
	"queues": {
		"consumers": [
			{
				"queue": "shopflare"
			}
		],
		"producers": [
			{
				"queue": "shopflare",
				"binding": "WEBHOOK_QUEUE"
			}
		]
	},
	"upload_source_maps": true,
	"vars": {
		"NODE_VERSION": 22,
		"SHOPIFY_API_KEY": "",
		"SHOPIFY_APP_HANDLE": "",
		"SHOPIFY_APP_URL": ""
	}
}
